          for (var cnt = arrayCounter; cnt >= 0; cnt--) {
              if (wordCount[cnt][1] == maxfreq) {
                   output += wordCount[cnt][0];
                   output += " ";
                   output += wordCount[cnt][1];
                   output += "\n";
              }
          }
          maxfreq -= 1;
       }
       return resultElement.textContent = 'The frequency of the words are : ' + output;
      
